 <div id="project">
<h1>Edit Project</h1>
 
<% @existing_project.errors.full_messages.each do |msg| %>
	<p><%= msg %></p>
<% end %>
 
<!-- Creates a PUT request to "/users/1/edit" -->
<%#= form_for(@existing_recruit, :as => "user", :url => user_path(@existing_recruit.id), :method => "put") ... %>
 
<%= form_for (@existing_project) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	
	<%= f.label :summary %>
	<%= f.text_field :summary %>
	
	<%= f.label :code_language %>
	<%= f.text_field :code_language %>
	
	<%= f.label :github %>
	<%= f.text_field :github %>
	
	<%= f.submit %>
	
<% end %>

<p><%= link_to("destroy", articles_path, :method => :delete) %></p>
<p><%= link_to 'Go Home', root_path %></p>


</div>