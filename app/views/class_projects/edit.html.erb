<h1>Edit Project</h1>

<% @class_project.errors.full_messages.each do |msg| %>
  <p><%= msg %></p>
<% end %>

<!-- Creates a PUT request to "/users/1/edit" -->
<%#= form_for(@existing_recruit, :as => "user", :url => user_path(@existing_recruit.id), :method => "put") ... %>

<%= form_for(@class_project, :url => "/classprojects/#{@class_project.id}" do |f| %>

   <%= f.label :name %>
   <%= f.text_field :name %>
 
   <%= f.label :code_language %>
   <%= f.text_field :code_language %>
 
   <%= f.label :summary %>
   <%= f.text_field :summary %>
 
   <%= f.label :link %>
   <%= f.text_field :link %>
  
  <%= f.submit %>

<% end %>
