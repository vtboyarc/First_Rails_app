 <div id="contact">
<h1>New Contact!</h1>

<% @contact.errors.full_messages.each do |msg| %>
  <p><%= msg %></p>
<% end %>

<!-- Creates a POST request to "/users" -->
<%= form_for @contact do |f| %>

   <%= f.label :name %>
   <%= f.text_field :name %>
 
   <%= f.label :email %>
   <%= f.text_field :email %>
 
   <%= f.label :message %>
   <%= f.text_field :message %>

  <%= f.submit %>

<% end %>

<p><%= link_to 'Go Home', root_path %></p>

</div>
 

